{
    "compilerOptions": {
        "target": "es5",
        "module": "commonjs",
        "moduleResolution": "node",
        "isolatedModules": false,
        "jsx": "react",
        "experimentalDecorators": true,
        "emitDecoratorMetadata": true,
        "declaration": false,
        "noImplicitAny": true,
        "removeComments": false,
        "noLib": false,
        "preserveConstEnums": true,
        "suppressImplicitAnyIndexErrors": true
    },
    "filesGlob": [
        "./**/*.ts",
        "./**/*.tsx",
        "!./node_modules/typescript/**/*.ts"
    ],
    "compileOnSave": false,
    "files": [
        "src/app.ts",
        "src/decorators/decorators.spec.ts",
        "src/decorators/decorators.ts",
        "src/models/song.spec.ts",
        "src/models/song.ts",
        "src/typings/env.d.ts",
        "src/typings/vue-class-component.d.ts",
        "typings/bundle.d.ts",
        "typings/chai/chai.d.ts",
        "typings/jquery/jquery.d.ts",
        "typings/mocha/mocha.d.ts",
        "typings/node/node.d.ts",
        "typings/parse/parse.d.ts",
        "typings/sinon/sinon.d.ts",
        "typings/underscore/underscore.d.ts",
        "typings/vue/vue.d.ts"
    ],
    "exclude": []
}
